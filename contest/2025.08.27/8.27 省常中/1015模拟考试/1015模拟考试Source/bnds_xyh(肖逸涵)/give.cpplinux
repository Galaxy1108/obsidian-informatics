#include<bits/stdc++.h>
using namespace std;
const int NR=1e5+5;
int n,m,s,x;
void solve(){
	cin >> m >> s >> n;
	if(n==1){
		cin >> x;
		if(m*x==s) puts("1");
		else puts("0");
		return;
	}
	if(rand()%2) puts("0");
	else puts("1");
}
int main(){
	freopen("give.in","r",stdin);
	freopen("give.out","w",stdout);
	srand(time(0));
	int t;cin >> t;
	while(t--) solve();
	return 0;
}

