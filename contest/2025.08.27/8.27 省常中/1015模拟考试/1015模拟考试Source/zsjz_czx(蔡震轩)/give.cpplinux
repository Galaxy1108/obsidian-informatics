#include<bits/stdc++.h>
using namespace std;
const int N=205;
int n,m,S,T,a[N];
int main()
{
	freopen("give.in","r",stdin);
	freopen("give.out","w",stdout);
	scanf("%d",&T);
	for (;T--;)
	{
		scanf("%d%d%d",&n,&S,&m);
		for (int i=1;i<=n;++i) scanf("%d",&a[i]);
		if (n==1)
		{
			if (a[1]*m==S) puts("1"); else puts("0");
			continue;
		}
		puts("1");
	}
} 