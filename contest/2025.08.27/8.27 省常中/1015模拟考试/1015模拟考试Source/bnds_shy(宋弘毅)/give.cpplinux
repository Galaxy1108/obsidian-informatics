#include <fstream>
using namespace std;
ifstream cin( "give.in" );
ofstream cout( "give.out" );
int a[ 210 ];
int main()
{
    int T;
    cin >> T;
    while ( T-- )
    {
        int m, s, n;
        cin >> m >> s >> n;
        for ( int i = 1; i <= n; i++ )
        {
            cin >> a[ i ];
        }
        if ( m * a[ 1 ] != s )
        {
            cout << 0 << endl;
        }
        else
        {
            cout << 1 << endl;
        }
    }
}