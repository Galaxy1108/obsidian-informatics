#include<cstdio>
#include<cstring>
using namespace std;
const int N=1e3+100;
int n,m,s,a[N+10];
bool f[N+10][N+10];
int main()
{
	freopen("give.in","r",stdin);
	freopen("give.out","w",stdout);
	int T;
	scanf("%d",&T);
	for(;T--;)
	{
		scanf("%d %d %d",&m,&s,&n);
		memset(f,0,sizeof(f));
		f[0][0]=true;
		for(int i=1;i<=m;i++)
		{
			for(int j=1;j<=n;j++)
			{
				for(int k=a[j];k<=s;k++)
				{
					f[i][k]=f[i][k]+f[i-1][k-a[j]]&1;
				}
			}
		}
		printf("%d\n",f[m][s]);
	}
	return 0;
}