#include<bits/stdc++.h>
#define ll long long
using namespace std;
inline int read(){
   int s=0,w=1;
   char ch=getchar();
   while(ch<'0'||ch>'9'){if(ch=='-')w=-1;ch=getchar();}
   while(ch>='0'&&ch<='9') s=s*10+ch-'0',ch=getchar();
   return s*w;
}
int main(){
	ll t=read();
	while(t--){
		ll n=read(),s=read(),T=read(),x=read(),y=read();
		for(int i=1;i<=n;i++){
			if(s>=T) s-=x;
			else s+=y;
		}
		printf("%lld\n",s);
	} 
	return 0;
}
