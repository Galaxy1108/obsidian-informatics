#include<iostream>
#include<cstdio>
using namespace std;
const int MX=1e5+7;
int ans=0;
int n,m,a[MX]; 
void solve(int p,int s){
	if(p>m){
		if(s==0)ans=(ans+1)%2;
		return;
	}
	if(s<=0)return;
	for(int i=1;i<=n;i++)solve(p+1,s-a[i]);
}
int main(){
	freopen("give.in","r",stdin);
	freopen("give.out","w",stdout);
	int T;
	cin>>T;
	while(T--){
		int s;
		cin>>m>>s>>n;
		for(int i=1;i<=n;i++)cin>>a[i];
		ans=0;
		solve(1,s);
		cout<<ans<<endl;
	}
	return 0;
} 