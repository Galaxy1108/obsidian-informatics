#include <iostream>
using namespace std;
int T;
long long m,s,n,a;
int main()
{
	freopen("give.in","r",stdin);
	freopen("give.out","w",stdout);
	cin>>T;
	while(T--)
	{
		cin>>m>>s>>n;
		for(int i=1;i<=n;++i) cin>>a;
		if(n==1)
		{
			if(m*a==s) cout<<1<<endl;
			else cout<<0<<endl;
		}
		else if(n<=5) cout<<1<<endl;
		else cout<<0<<endl;
	}
	return 0;
}