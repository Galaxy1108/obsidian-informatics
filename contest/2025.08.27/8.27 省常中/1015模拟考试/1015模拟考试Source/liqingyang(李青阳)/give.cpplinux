#include<iostream>
#include<cstring>
using namespace std;
int T,n,m,s,a[210];
bool f[5010];
int main()
{
	freopen("give.in","r",stdin);
	freopen("give.out","w",stdout);
	cin>>T;
	while(T--)
	{
		cin>>m>>s>>n;
		for(int i=1;i<=n;i++)
		{
			cin>>a[i];
		}
		memset(f,0,sizeof(f));
		f[0]=1;
		for(int i=1;i<=m;i++)
		{
			for(int j=s;j>=0;j--)
			{
				bool F=0;
				for(int k=1;a[k]<=j&&k<=n;k++)
				{
					F^=f[j-a[k]];
				}
				f[j]=F;
			}
		}
		cout<<f[s]<<endl;
	}
	return 0;
}