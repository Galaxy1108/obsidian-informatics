#include <bits/stdc++.h>
using namespace std;
int p;
int fastpow(int a,int b)
{
	int ans = 1;
	while(b){
		if(b&1)ans = (long long)ans*a%p;
		a = (long long)a*a%p;
		b >>= 1;
	}
	return ans;
}
int main()
{
	freopen("easyhard.in","r",stdin);
	freopen("easyhard.in","w",stdout);
	int n,m;
	cin >> n >> m >> p;
	if(n == 1){
		cout << (m<<2)%p;
		return 0;
	}
	cout << fastpow(2,m<<1);
	return 0;
}