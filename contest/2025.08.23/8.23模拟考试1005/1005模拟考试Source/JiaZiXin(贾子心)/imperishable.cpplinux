#include<bits/stdc++.h>
using namespace std;
const int p=998244353;
int T,n,a[100005],b[100005],cnt,q;
vector<int> g[105];
#define ll long long 
ll qpow(ll a,ll b){
	ll ans=1;a%=p;
	for(;b;b>>=1){
		if(b&1)ans=ans*a%p;
		a=a*a%p;
	}
	return ans;
}
int main(){
	freopen("imperishable.in","r",stdin);
	freopen("imperishable.out","w",stdout);
	cin>>T>>n;
	for(int i=1;i<=n;i++){
		cin>>a[i];
		for(int j=1;j<=a[i];j++)b[++cnt]=i;
	}
	if(T==1){
		cout<<qpow(2ll,a[1])<<endl;
		return 0;
	}
}