#include<bits/stdc++.h>
using namespace std;
int n;
struct point
{
	int fa;int v;
	vector <int> son;
	vector <int> sv;
}tree[1000005];
int ans;
int main()
{
	freopen("treep.in","r",stdin);
	freopen("treep.out","w",stdout);
	cin>>n;
	int f,w;
	for(int i=2;i<=n;i++)
