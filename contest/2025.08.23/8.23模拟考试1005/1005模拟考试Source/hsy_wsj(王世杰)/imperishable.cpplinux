#include<bits/stdc++.h>
#define int long long
#define mo 998244353
using namespace std;
int num,n,a[200005],q;
int f[200005];
int ans;
signed main()
{
	freopen("imperishable.in","r",stdin);
	freopen("imperishable.out","w",stdout);
	cin>>num>>n;
	for(int i=1;i<=n;i++)
	{
		scanf("%d",&a[i]);
	}
	if(n<=1) cout<<1<<endl;
	else
	{
		cout<<rand()%mo+1;
	}
	cin>>q;
	int x,y;
	for(int i=1;i<=q;i++)
	{
		scanf("%d%d",&x,&y);
		a[x]=y;
		cout<<rand()%mo+1;
	}
	return 0;
}