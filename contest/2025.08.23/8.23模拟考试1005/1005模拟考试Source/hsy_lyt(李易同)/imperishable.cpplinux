#include <iostream>
using namespace std;
int n;
int a[200010];
int q;
int x,y;
int read()
{
	int s=0;
	char ch=getchar();
	while(!isdigit(ch)) ch=getchar();
	while(isdigit(ch))
	{
		s=(s<<3)+(s<<1)+(ch^48);
		ch=getchar();
	}
	return s;
}
int main()
{
	freopen("imperishable.in","r",stdin);
	freopen("imperishable.out","w",stdout);
	n=read();
	n=read();
	for(int i=1;i<=n;++i) a[i]=read();
	q=read();
	for(int i=1;i<=q;++i)
	{
		x=read();
		y=read();
	}
	cout<<(1<<a[1]);
	return 0;
}