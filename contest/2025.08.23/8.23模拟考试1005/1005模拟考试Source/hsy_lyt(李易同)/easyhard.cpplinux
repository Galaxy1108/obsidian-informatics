#include <iostream>
using namespace std;
int n,m,p;
int ans;
int qpow(long long a,int b)
{
	long long res=1;
	while(b)
	{
		if(b&1) res=res*a%p;
		b>>=1;
		a=a*a%p;
	}
	return res;
}
int main()
{
	freopen("easyhard.in","r",stdin);
	freopen("easyhard.out","w",stdout);
	cin>>n>>m>>p;
	if(n>=m) return cout<<qpow(4,m),0;
	else return cout<<((qpow(4,m)-qpow(4,m-n)*2)%p+p)%p,0;
	return 0;
}