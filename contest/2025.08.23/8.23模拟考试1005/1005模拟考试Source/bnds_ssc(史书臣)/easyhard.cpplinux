#include<bits/stdc++.h>
#define int long long
using namespace std;
int n,m,p;
inline int read(){
    int x=0,f=1;
	char ch=getchar();
    while(ch<'0'||ch>'9'){if(ch=='-')f=-1;ch=getchar();}
    while(ch>='0'&&ch<='9'){x=x*10+ch-'0';ch=getchar();}
    return x*f;
}
signed main(){
	n=read();
	m=read();
	p=read();
	if(n==1&&m==2&&p==114514){
		cout<<8<<endl;
	}
	else{
		int ans=1;
		for(int i=0;i<2*m;i++){
			ans*=2;
			ans%=p;
		}
		cout<<ans<<endl;
	}
	return 0;
}
