#include<iostream>
#include<cmath>
using namespace std;
typedef long long ll;
ll QuickPow(ll x,ll a,ll p)
{
	ll base=x,ans=1;
	while(a>0)
	{
		if(a&1)
			ans=(base*ans)%p;
		base=(base*base)%p;
		a>>=1;
	}
	return ans;
}
int main()
{
	ll n,m,p,ans;
	scanf("%lld%lld%lld",&n,&m,&p);
	ans=QuickPow(2,n,p)*QuickPow(2,m,p)%p;
	printf("%lld",ans);
	return 0;
}