#include<bits/stdc++.h>
using namespace std;
long long n,m,p,dp[3001][3001];
int main(){
	freopen("easyhard.in","r",stdin);
	freopen("easyhard.out","w",stdout);
	scanf("%lld%lld%lld",&n,&m,&p);
	for(int i=1;i<=n;i++) dp[i][1]=4%p;
	for(int i=2;i<=m;i++){
		dp[1][i]=dp[1][i-1]*2%p;
		for(int j=1;j<=n;j++) dp[j][i]=(dp[j-1][i-1]+dp[j][i-1]*3)%p;
	}
	printf("%lld",dp[n][m]);
	return 0;
}