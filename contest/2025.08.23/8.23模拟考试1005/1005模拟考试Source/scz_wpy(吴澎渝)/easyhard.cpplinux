#include <bits/stdc++.h>
using namespace std;
#define ll long long
ll n,m,mod;
int main()
{
	freopen("easyhard.in","r",stdin);
	freopen("easyhard.out","w",stdout);
	cin>>n>>m>>mod;
	if(n>=m)
	{
		ll ans=1;
		for(int i=1;i<=2*m;i++)
		{
			ans*=2;
			ans%=mod;
		}
		cout<<ans<<endl;
		return 0;
	}
	ll ans=1;
	for(int i=1;i<=n;i++)
	{
		ans*=2;
		ans%=mod;
	}
	for(int j=1;j<=m;j++)
	{
		ans*=2;
		ans%=mod;
	}
	cout<<ans<<endl;
	return 0;
} 