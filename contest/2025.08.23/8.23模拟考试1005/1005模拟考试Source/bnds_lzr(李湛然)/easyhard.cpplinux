                cout<<quickpow(2,m+1);
            
            }
    if(m<=10)
    {
            for(int i=0;i<(1<<(2*m));i++)
        {
            one=0;
            two=0;
            if(check(i))
            {
                //cout<<"Yes "<<i<<endl;
                ans++;
                ans%=p;
            }
        }
        cout<<ans%p;
        }
        else
        {
