#include<bits/stdc++.h>
#define for1(i,a,b) for(int i=a;i<=b;i++)
using namespace std;
const int N=2e5+10;
int n,m,p,ans=0;
void dfs(int b,int w,int step)
{
	if(step==m)
	{
		ans=(ans+1)%p;
		return;
	}
	dfs(b,w,step+1);
	if(b==0) dfs(b+1,w-1,step+1);
	else if(w==0) dfs(b-1,w+1,step+1);
	else
	{
		dfs(b-1,w+1,step+1);
		dfs(b+1,w-1,step+1);
	}
}
int main()
{
	//freopen(".in","r",stdin);
	//freopen(".out","w",stdout);
	cin>>n>>m>>p;
	for1(i,0,n) dfs(i,n-i,0);
	cout<<ans;
	return 0;
}