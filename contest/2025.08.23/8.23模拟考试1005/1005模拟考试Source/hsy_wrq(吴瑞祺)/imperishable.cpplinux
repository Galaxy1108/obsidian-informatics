#include<bits/stdc++.h>
using namespace std;
int a,b,c;
long long fastpow(long long a,long long b,long long p){
	long long ans=1,base=a;
	for(;b;b>>=1){
		if(b&1) ans=(ans*base)%p;
		base=base*base%p;
	}
	return ans;
}
int main(){
	freopen("imperishable.in","r",stdin);
	freopen("imperishable.out","w",stdout);
	cin>>a>>b>>c;
	cout<<fastpow(2,c,998244353);
	return 0;
}