#include<bits/stdc++.h>
#define ll long long
using namespace std;
int n,m,p;
ll ksm(int a,int b){
	if(b==0) return 1;
	else if (b%2==1)
        return ksm(a,b-1)*a%p;
    else
    {
        ll temp=ksm(a,b/2)%p;
        return temp*temp%p;
    }
}
int main(){
	freopen("easyhard.in","r",stdin);
	freopen("easyhard.out","w",stdout);
	cin>>n>>m>>p;
	ll ans1=ksm(4,n)%p;
	ll ans2=ksm(2,m-n)%p;
	ll ans=ans1*ans2%p;
	cout<<ans;
	return 0;
}