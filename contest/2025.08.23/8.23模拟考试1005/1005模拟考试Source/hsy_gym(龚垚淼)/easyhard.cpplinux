#include<iostream>
#define ll long long
using namespace std;
int n,m;
int p;
inline ll fpow(ll x,int k)
{
	ll res(1);
	while(k)
	{
		if(k&1)res=(res*x)%p;
		x=(x*x)%p;
		k>>=1;
	}
	return res;
}
int main()
{
	ios::sync_with_stdio(false);
	cin.tie(0),cout.tie(0);
	freopen("easyhard.in","r",stdin);
	freopen("easyhard.out","w",stdout);
	cin>>n>>m>>p;
	cout<<fpow(2,n+m);
	return 0;
} 