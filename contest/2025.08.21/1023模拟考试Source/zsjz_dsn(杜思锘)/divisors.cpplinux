#include<iostream>
#include<cstdio>
using namespace std;
int n;
int main(){
	freopen("divisors.in","r",stdin);
	freopen("divisors.out","w",stdout);
	scanf("%d",&n);
	for(int i=1;i<=n;i++){
		if(i<n) printf("%d ",i*(i+1));
		else printf("%d\n",i);
	}
	for(int i=1;i<n;i++){
		printf("%d %d\n",i,i+1);
	}
	return 0;
}