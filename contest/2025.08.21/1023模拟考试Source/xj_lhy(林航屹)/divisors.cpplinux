#include <cstdio>
#include <iostream>
using namespace std;

int main()
{
	freopen("divisors.in", "r", stdin);
	freopen("divisors.out", "w", stdout);
	int n, top = 1; scanf("%d", &n);
	for (int i = 1; i <= n; ++ i) top *= i;
	printf("%d ", top);
	for (int i = 2; i <= n; ++ i) printf("%d ", i);
	puts("");
	for (int i = 2; i <= n; ++ i) printf("1 %d\n", i);
	return 0;
}