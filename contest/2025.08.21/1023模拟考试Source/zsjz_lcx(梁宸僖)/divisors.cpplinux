#include<cstdio>
using namespace std;
int gcd(int x,int y){return !y?x:gcd(y,x%y);}
int n,ret;
int main()
{
	freopen("divisors.in","r",stdin);
	freopen("divisors.out","w",stdout);
	scanf("%d",&n);
	ret=1;
	for (int i=2;i<=n;i++)
		ret=ret*i/gcd(ret,i);
	printf("%d ",ret);
	for (int i=2;i<=n;i++)
		printf("%d ",i);
	puts("");
	for (int i=2;i<=n;i++)
		printf("%d %d\n",1,i);
	return 0;
}