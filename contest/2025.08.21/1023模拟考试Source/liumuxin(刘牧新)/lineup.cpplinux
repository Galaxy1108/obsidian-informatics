#include <bits/stdc++.h>
using namespace std;
int a[2020], b[2202];
int main() {
	freopen("lineup.in", "r", stdin);
	freopen("lineup.out", "w", stdout);
	int n;
	cin >> n;
	int ans = 0;
	for(int i = 1; i <= n; i++) cin >> a[i];
	for(int i = 1; i <= n; i++) {
		b[i] = a[i];
	}
	sort(a + 1, a + n + 1);
	for(int i = 1; i <= n; i++) {
		for(int j = 1; j <= n; j++) {
			if(b[j] == a[i]) {
				ans += abs(i - j);
				b[j] = 0;
				break;
			}
		}
	}
	cout << ans << endl;
	return 0;
}