#include <cstdio>
using namespace std;
int n;
int main()
{
	freopen("divisors.in","r",stdin);
	freopen("divisors.out","w",stdout);
	scanf("%d",&n);
	for (int i = 2,j = n;i <= j;i++,j--)
	{
		if (i == 2)
			printf("2 ");
		else
			printf("%d ",i * (j + 1));
		if (i == j)
			printf("%d",i);
		else
			printf("%d ",i * j);
	}
	if (n & 1)
		printf("%d",(n + 3) >> 1);
	printf("\n");
	for (int i = 1;i <= n - 1;i++)
		printf("%d %d\n",i,i + 1);
	return 0;
}