#include<iostream>
#include<cstdio>
#include<cmath>
#include<algorithm>
#define R register
using namespace std;
const int N=1e5+5;
int n,h[N],h2[N];
int main()
{
	freopen("lineup.in","r",stdin);freopen("lineup.out","w",stdout);
	scanf("%d",&n);
	for (int i=1;i<=n;++i) scanf("%d",&h[i]),h2[i]=h[i];
	sort(h2+1,h2+1+n);
	int ans=0;
	for (int i=1;i<=n;++i)
		for (int j=1;j<=n;++j)
			if (h[i]==h2[j])
			{
				h[i]=h2[j]=0;
				ans+=abs(i-j);
				break;
			}
	printf("%d",ans);
	return 0;
}
