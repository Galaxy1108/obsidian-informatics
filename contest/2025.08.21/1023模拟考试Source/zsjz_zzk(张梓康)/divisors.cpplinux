#include<cstdio>
using namespace std;
int n,s=1;
int gcd(int a,int b)
{
	int t;
	while(b!=0)
	{
		t=a%b;
		a=b;
		b=t;
	}
	return a;
}
int main()
{
	freopen("divisors.in","r",stdin);
	freopen("divisors.out","w",stdout);
	int i;
	scanf("%d",&n);
	for(i=1;i<=n;++i)
	  s=s*i/gcd(s,i);
	printf("%d",s);
	for(i=2;i<=n;++i)
	  printf(" %d",i);
	putchar(10);
	for(i=2;i<=n;++i)
	  printf("1 %d\n",i);
	return 0;
}